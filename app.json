{
  "expo": {
    "name": "Bybus",
    "slug": "viajes",
    "version": "0.1.3",
    "orientation": "portrait",
    "icon": "./src/utils/images/logo_app.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/utils/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00B4D8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/405c0b8f-173f-4074-9680-cad80d10c052"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "runtimeVersion": "exposdk:48.0.0",
      "supportsTablet": true
    },
    "android": {
      "runtimeVersion": "exposdk:48.0.0",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.bybusvenezuela.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.bybus.viajes",
      "adaptiveIcon": {
        "foregroundImage": "./src/utils/images/logo_app.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.gms.permission.AD_ID"
      ],
      "versionCode": 8
    },
    "web": {
      "favicon": "./src/utils/images/logo_app.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "css"]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Espero no tengas nudes"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "405c0b8f-173f-4074-9680-cad80d10c052"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
